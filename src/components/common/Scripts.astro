<!-- GSAP Scripts -->
<script is:inline src="https://cdnjs.cloudflare.com/ajax/libs/gsap/3.12.2/gsap.min.js"></script>
<script is:inline src="https://cdnjs.cloudflare.com/ajax/libs/gsap/3.12.2/MotionPathPlugin.min.js"></script>

<!-- Initialize GSAP -->
<script is:inline>
  // Guaranteed to run once
  window.gsapInitialized = window.gsapInitialized || false;
  if (!window.gsapInitialized) {
    gsap.registerPlugin(MotionPathPlugin);
    window.gsapInitialized = true;
  }
</script>
